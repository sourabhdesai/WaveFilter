    "id3v2-parser": "~0.3.0",
    "fuzzysearch": "~1.0.3",
    "musicmetadata": "~1.0.1",
    "echonest": "0.0.3",
    "echojs": "~0.1.4",
    "id3_reader": "~1.0.1",
    "request": "~2.55.0",
